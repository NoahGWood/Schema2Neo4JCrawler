[Recipe(additionalType=None, alternateName=None, description=None, disambiguatingDescription=None, identifier=None, image=[
        {'@type': 'ImageObject', 'url': 'https: //food.fnr.sndimg.com/content/dam/images/food/fullset/2008/10/21/0/FNMTomatoGorgonzolaSoup_s3x4.jpg.rend.hgtvcom.406.406.suffix/1371587394510.jpeg', 'height': '406', 'width': '406'}, {'@type': 'ImageObject', 'url': 'https://food.fnr.sndimg.com/content/dam/images/food/fullset/2008/10/21/0/FNMTomatoGorgonzolaSoup_s3x4.jpg.rend.hgtvcom.406.305.suffix/1371587394510.jpeg', 'height': '305', 'width': '406'}, {'@type': 'ImageObject', 'url': 'https://food.fnr.sndimg.com/content/dam/images/food/fullset/2008/10/21/0/FNMTomatoGorgonzolaSoup_s3x4.jpg.rend.hgtvcom.406.229.suffix/1371587394510.jpeg', 'height': '229', 'width': '406'}], mainEntityOfPage=True, name='Tomato Gorgonzola Soup', potentialAction=None, sameAs=None, subjectOf=None, url='https://www.foodnetwork.com/recipes/tomato-gorgonzola-soup-recipe-2103464', about=None, abstract=None, accessMode=None, accessModeSufficient=None, accessibilityAPI=None, accessibilityControl=None, accessibilityFeature=None, accessibilityHazard=None, accessibilitySummary=None, accountablePerson=None, acquireLicensePage=None, aggregateRating={'@type': 'AggregateRating', 'ratingValue': 4.6, 'reviewCount': 19}, alternativeHeadline=None, archivedAt=None, assesses=None, associatedMedia=None, audience=None, audio=None, author={'@type': 'Person', 'name': 'Food Network'}, award=None, character=None, citation=None, comment=None, commentCount=None, conditionsOfAccess=None, contentLocation=None, contentRating=None, contentReferenceTime=None, contributor=None, copyrightHolder=None, copyrightNotice=None, copyrightYear=None, correction=None, countryOfOrigin=None, creativeWorkStatus=None, creator=None, creditText=None, dateCreated=None, dateModified='2013-11-08T09:30:53.068-05:00', datePublished='2016-11-20T23:38:07.856-05:00', discussionUrl=None, editEIDR=None, editor=None, educationalAlignment=None, educationalLevel=None, educationalUse=None, encoding=None, encodingFormat=None, exampleOfWork=None, expires=None, funder=None, funding=None, genre=None, hasPart=None, headline='Tomato Gorgonzola Soup', inLanguage=None, interactionStatistic=None, interactivityType=None, interpretedAsClaim=None, isAccessibleForFree=None, isBasedOn=None, isFamilyFriendly=None, isPartOf=None, keywords='Tomato Soup,Vegetable Soup,Soup,Tomato,Vegetable,High Fiber', learningResourceType=None, license=None, locationCreated=None, mainEntity=None, maintainer=None, material=None, materialExtent=None, mentions=None, offers=None, pattern=None, position=None, producer=None, provider=None, publication=None, publisher={'@type': 'Organization', 'name': 'Food Network', 'url': 'https://www.foodnetwork.com', 'logo': {'@type': 'ImageObject', 'url': 'https://food.fnr.sndimg.com/etc/clientlibs/assets/images/food/fn-logo-flat-60x60.png', 'height': '60', 'width': '60'}}, publisherImprint=None, publishingPrinciples=None, recordedAt=None, releasedEvent=None, review=[{'@type': 'Review', 'author': {'@type': 'Person', 'name': 'Monica Ausderau Larmon'}, 'reviewRating': {'@type': 'Rating', 'ratingValue': 5, 'worstRating': '1', 'bestRating': '5'}, 'reviewBody': "I first tasted this at Sack's Cafe more than 6 years ago.  Being one of my favourite restaurants, I was very excited to see that the recipe was online. <br /><br />I've been making it for the past 5 years, and one thing I changed is adding about 2.5oz of tomato paste (half a small can-freeze other half for next time I make it), and that makes a huge difference in the tomato flavour.  So if you find that this isn't tomato-ey enough for you, give that a try.  <br /><br />I've changed the cream a bit depending on what I have on hand at the time.  I've used varying amounts of milk, cream, and half &amp; half with great success.", 'datePublished': '2014-09-07'}, {'@type': 'Review', 'author': {'@type': 'Person', 'name': 'mrstdw'}, 'reviewRating': {'@type': 'Rating', 'ratingValue': 1, 'worstRating': '1', 'bestRating': '5'}, 'reviewBody': 'Awful. Bland and tasteless. If it were not pink you would never know there were tomatoes in it. Went down the drain.', 'datePublished': '2012-10-28'}, {'@type': 'Review', 'author': {'@type': 'Person', 'name': 'muddydan'}, 'reviewRating': {'@type': 'Rating', 'ratingValue': 5, 'worstRating': '1', 'bestRating': '5'}, 'reviewBody': "Outstanding.  I'm not a big fan of tomato soups in general but made this for my wife and daughter.  Followed advice from reviews already posted, added about 3/4 cups 2% milk and 2 cups cream instead of all cream and a spoonful of tomato paste for a little more tomato flavor.  Absolutely delicious.  Definitely a little extra crumbled gorgonzola on top finishes it off nicely.", 'datePublished': '2011-12-27'}, {'@type': 'Review', 'author': {'@type': 'Person', 'name': 'Amy_Alise'}, 'reviewRating': {'@type': 'Rating', 'ratingValue': 5, 'worstRating': '1', 'bestRating': '5'}, 'reviewBody': "This is probably my favorite soup to make!! I've made it a few times now and I think everytime, I like it a little bit more! I had a dinner party a few months ago and this soup stole the show, there wasn't a drop left! I personally use a little bit more gorgonzola to top each bowl off with.", 'datePublished': '2011-09-06'}, {'@type': 'Review', 'author': {'@type': 'Person', 'name': 'Sanjolisa'}, 'reviewRating': {'@type': 'Rating', 'ratingValue': 5, 'worstRating': '1', 'bestRating': '5'}, 'reviewBody': 'This was great!!! Really good flavor, very hearty.  I made it almost exactly as written except used vegetable stock and only added 1 cup of cream.  It was perfect.  Super comfort food.  mmmm', 'datePublished': '2011-03-14'}, {'@type': 'Review', 'author': {'@type': 'Person', 'name': 'Familyof4xo'}, 'reviewRating': {'@type': 'Rating', 'ratingValue': 5, 'worstRating': '1', 'bestRating': '5'}, 'reviewBody': 'I am sooooo excited to have stumbled onto this recipe. I have grown up eating at Saks Cafe and it is by far my favorite restaurant in Anchorage. This soup is an amazing Tomato soup,that I enjoy at anytime. I am so glad they shared. I can not wait to make it. Cheers !', 'datePublished': '2010-12-08'}, {'@type': 'Review', 'author': {'@type': 'Person', 'name': 'Lisa S.'}, 'reviewRating': {'@type': 'Rating', 'ratingValue': 5, 'worstRating': '1', 'bestRating': '5'}, 'reviewBody': "I've made this soup a couple of times and it always turns out great. I left out the cognac because it isn't something that I had around. Warning: cool the soup before using a regular blender to blend. I didn't the first time I made this and the pressure from the heat caused the top of the blender to be blown off and soup to go everywhere. ", 'datePublished': '2010-07-01'}, {'@type': 'Review', 'author': {'@type': 'Person', 'name': 'jannifer m.'}, 'reviewRating': {'@type': 'Rating', 'ratingValue': 5, 'worstRating': '1', 'bestRating': '5'}, 'reviewBody': 'My first attempt at tomato soup and it was delish! ', 'datePublished': '2010-02-12'}, {'@type': 'Review', 'author': {'@type': 'Person', 'name': 'michelle p.'}, 'reviewRating': {'@type': 'Rating', 'ratingValue': 3, 'worstRating': '1', 'bestRating': '5'}, 'reviewBody': "In the picture this soup is yellow, but when I made the soup it looked like puke. It was pink with bits of other stuff in it. Did anybody else's soup turn puke pink? It tastes ok, but it is not very appetizing to look at.", 'datePublished': '2010-01-17'}, {'@type': 'Review', 'author': {'@type': 'Person', 'name': 'Ashley D.'}, 'reviewRating': {'@type': 'Rating', 'ratingValue': 5, 'worstRating': '1', 'bestRating': '5'}, 'reviewBody': 'I made this soup for a church lunch and got rave reviews.  So many people asked for the recipe!  I followed the recipe but found that it could use a bit more tomato flavor.  Next time I will try adding some tomato paste.  I will definitely be making this soup again.\r<br />', 'datePublished': '2009-12-17'}], schemaVersion=None, sdDatePublished=None, sdLicense=None, sdPublisher=None, size=None, sourceOrganization=None, spatial=None, spatialCoverage=None, sponsor=None, teaches=None, temporal=None, temporalCoverage=None, text=None, thumbnail=None, thumbnailUrl=None, timeRequired=None, translationOfWork=None, translator=None, typicalAgeRange=None, usageInfo=None, version=None, video=None, workExample=None, workTranslation=None, estimatedCost=None, performTime=None, prepTime='P0Y0M0DT0H20M0.000S', step=None, supply=None, tool=None, totalTime='P0Y0M0DT0H55M0.000S', cookTime='P0Y0M0DT0H35M0.000S', cookingMethod=None, nutrition={'@type': 'NutritionInformation', 'servingSize': '1 of 6 servings', 'calories': '684', 'fatContent': '52g', 'saturatedFatContent': '32g', 'carbohydrateContent': '40g', 'fiberContent': '7g', 'sugarContent': '11g', 'proteinContent': '14g', 'cholesterolContent': '176mg', 'sodiumContent': '1492mg'}, recipeCategory=None, recipeCuisine=None, recipeIngredient=['3 tablespoons unsalted butter', '2 large yellow onions, chopped', '4 cloves garlic, chopped', '3 tablespoons all-purpose flour', '2 ounces (1/4 cup) cognac', '1 1/2 pounds red-skinned potatoes, peeled and quartered', '4 cups low-sodium chicken broth', '1 35 -ounce can whole peeled tomatoes, crushed, juices reserved', '2 3/4 cups heavy cream', '2/3 cup crumbled gorgonzola cheese, plus more for garnish', '2/3 cup fresh basil, plus more for garnish', 'Kosher salt and freshly ground pepper', '2 teaspoons cider vinegar'], recipeInstructions=[{'@type': 'HowToStep', 'text': 'Melt the butter in a large stockpot over medium heat. Add the onions and cook until softened, about 5 minutes. Add the garlic and cook until fragrant, about 2 minutes. Add the flour and cook until lightly toasted, about 2 minutes. Add the cognac and scrape the pan with a wooden spoon to release any browned bits.'}, {'@type': 'HowToStep', 'text': 'Add the potatoes and broth to the stockpot and bring to a boil. Reduce the heat and simmer until the potatoes are tender, about 15 minutes.'}, {'@type': 'HowToStep', 'text': 'Add the tomatoes and their juices, cream, gorgonzola and basil. Season with salt and pepper. Using an immersion blender, or in batches in a blender, puree until smooth. Add the vinegar and serve in warm bowls with more basil and gorgonzola.'}], recipeYield='Serves 4 to 6', suitableForDiet=None), BreadcrumbList(additionalType=None, alternateName=None, description=None, disambiguatingDescription=None, identifier=None, image=None, mainEntityOfPage=None, name=None, potentialAction=None, sameAs=None, subjectOf=None, url=None, itemListElement=[{'@type': 'ListItem', 'position': 1, 'item': {'@id': 'https://www.foodnetwork.com', 'name': 'Home'}}, {'@type': 'ListItem', 'position': 2, 'item': {'@id': 'https://www.foodnetwork.com/recipes', 'name': 'Recipes'}}], itemListOrder=None, numberOfItems=None), Article(additionalType=None, alternateName=None, description=None, disambiguatingDescription=None, identifier=None, image=None, mainEntityOfPage=None, name='Lockstep (computing)', potentialAction=None, sameAs='http://www.wikidata.org/entity/Q7663509', subjectOf=None, url='https://en.wikipedia.org/wiki/Lockstep_(computing)', about=None, abstract=None, accessMode=None, accessModeSufficient=None, accessibilityAPI=None, accessibilityControl=None, accessibilityFeature=None, accessibilityHazard=None, accessibilitySummary=None, accountablePerson=None, acquireLicensePage=None, aggregateRating=None, alternativeHeadline=None, archivedAt=None, assesses=None, associatedMedia=None, audience=None, audio=None, author={'@type': 'Organization', 'name': 'Contributors to Wikimedia projects'}, award=None, character=None, citation=None, comment=None, commentCount=None, conditionsOfAccess=None, contentLocation=None, contentRating=None, contentReferenceTime=None, contributor=None, copyrightHolder=None, copyrightNotice=None, copyrightYear=None, correction=None, countryOfOrigin=None, creativeWorkStatus=None, creator=None, creditText=None, dateCreated=None, dateModified='2023-04-19T04:59:35Z', datePublished='2005-03-31T22:46:31Z', discussionUrl=None, editEIDR=None,